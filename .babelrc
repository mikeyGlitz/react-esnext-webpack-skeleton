{
  "sourceMaps": false,
  "presets": [
    [
      "airbnb",
      {
        "modules": false
      }
    ],
    "react"
  ],
  "plugins": [
    "transform-class-properties",
    "syntax-async-functions",
    "lodash"
  ],
  "env": {
    "development": {
      "sourceMaps": true,
      "plugins": [
        "react-hot-loader/babel"
      ]
    },
    "test": {
      "presets": [
        "airbnb",
        "react"
      ],
      "sourceMaps": true
    }
  }
}