{
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "**/*.jsx",
      "!node_modules/**",
      "!public/**",
      "!docs/**",
      "!coverage/**",
      "!__tests__/**",
      "!.webpack-config/**",
      "!webpack.config.js"
    ],
    "unmockedModulePathPatterns": [
      "node_modules/react/",
      "node_modules/enzyme/"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "/__tests__/.*\\.(js|jsx)$"
  }