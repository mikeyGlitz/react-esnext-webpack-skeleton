{
    "source": "./",
    "destination": "./docs",
    "excludes": [
        "node_modules",
        "coverage",
        "__tests__",
        "docs",
        "\\.webpack-config",
        "webpack.config.js$",
        "public"
    ],
    "coverage": true,
    "manual": {
        "index": "README.md"
    },
    "experimentalProposal": {
        "classProperties": true,
        "objectRestSpread": true
    },
    "plugins": [
        {
            "name": "esdoc-flow-plugin"
        }
    ]
}