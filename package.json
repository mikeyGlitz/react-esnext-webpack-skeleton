{
  "name": "react-esnext-skeleton",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "webpack --config config/webpack.config.js",
    "dev-server": "webpack-dev-server --config config/webpack.config.js",
    "test": "jest",
    "docs": "esdoc . -c config/esdoc.json",
    "lint": "eslint .",
    "lint:styles": "stylelint **/*.css"
  },
  "jest": {
    "setupFiles": ["<rootDir>/setupTests.js"],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.js",
      "**/*.jsx",
      "!node_modules/**",
      "!public/**",
      "!docs/**",
      "!coverage/**",
      "!__tests__/**",
      "!.webpack-config/**",
      "!webpack.config.js"
    ],
    "moduleNameMapper": {
      "^.+\\.css$": "identity-obj-proxy"
    },
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "bluebird": "^3.5.0",
    "prop-types": "^15.5.10",
    "react": "^16.1.1",
    "react-dom": "^16.1.1",
    "regenerator-runtime": "^0.10.5",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "@easy-webpack/config-generate-index-html": "^2.1.1",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^20.0.3",
    "babel-loader": "^6.2.10",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-class-properties": "^6.19.0",
    "babel-preset-airbnb": "^2.4.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.1",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "esdoc": "^0.5.2",
    "eslint": "^3.9.1",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.9.0",
    "exports-loader": "^0.6.4",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.28.0",
    "identity-obj-proxy": "^3.0.0",
    "imagemin-webpack-plugin": "^1.4.4",
    "imports-loader": "^0.7.1",
    "jest": "^21.2.1",
    "jsdoc": "^3.4.3",
    "json-loader": "^0.5.4",
    "lodash-webpack-plugin": "^0.10.7",
    "lost": "^8.2.0",
    "postcss": "^6.0.2",
    "postcss-animations": "^0.3.0",
    "postcss-cssnext": "^2.11.0",
    "postcss-import": "^11.0.0",
    "postcss-loader": "^2.0.6",
    "postcss-mixins": "^6.2.0",
    "react-hot-loader": "3.0.0-beta.6",
    "react-test-renderer": "^15.6.1",
    "style-loader": "^0.13.1",
    "stylelint": "^7.11.1",
    "stylelint-config-lost": "^0.0.3",
    "stylelint-config-standard": "^16.0.0",
    "url-loader": "^0.5.7",
    "webpack": "^3.4.1",
    "webpack-dev-server": "^2.2.1"
  }
}
